import java.util.Scanner;
public class Main{
    
    
    public static int count(int n){
        int rem=0;
        while(n>0){
            n=n/10;
            rem++;
            
        }
        return rem;
    }

    

    
    
	public static void main(String[] args) {
		 Scanner sc= new Scanner(System.in);
         int n= sc.nextInt();
         System.out.println(count(n));
         
	}
}
